<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.mainChatControl">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Label layoutX="14.0" layoutY="14.0" prefHeight="31.0" prefWidth="176.0" text="ChaSe: Preferences">
               <font>
                  <Font size="21.0" />
               </font>
            </Label>
            <Button fx:id="saveButton" layoutX="551.0" layoutY="351.0" mnemonicParsing="false" onAction="#saveButtonHandler" prefHeight="25.0" prefWidth="75.0" text="Close" AnchorPane.leftAnchor="551.0" AnchorPane.topAnchor="351.0" />
            <Button fx:id="connectButton" layoutX="12.0" layoutY="122.0" mnemonicParsing="false" onAction="#connectToUserHandler" prefHeight="25.0" prefWidth="176.0" text="Connect" />
            <Label layoutX="14.0" layoutY="72.0" prefHeight="17.0" prefWidth="114.0" text="Name of User" />
            <Label layoutX="14.0" layoutY="51.0" prefHeight="21.0" prefWidth="176.0" text="Connection Settings:">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <TextArea fx:id="ChaSeConsole" editable="false" layoutX="11.0" layoutY="161.0" prefHeight="215.0" prefWidth="527.0" AnchorPane.leftAnchor="11.0" AnchorPane.topAnchor="161.0" />
            <Label layoutX="14.0" layoutY="378.0" text="ChaSe Console" AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="378.0" />
            <TextField fx:id="usernameField" layoutX="12.0" layoutY="89.0" prefHeight="25.0" prefWidth="176.0" />
      </children>
    </AnchorPane>
  </children>
</VBox>
